@Article{Javaraiah2020,
author={Javaraiah, J.},
title={Long--Term Variations in Solar Differential Rotation and Sunspot Activity, II: Differential Rotation Around the Maxima and Minima of Solar Cycles 12{\thinspace}--{\thinspace}24},
journal={\solphys},
year={2020},
month={Dec},
day={03},
volume={295},
number={12},
pages={170},
abstract={Studies of variations in the solar differential rotation are important for understanding the underlying mechanism of solar cycle and other variations of solar activity. We analyzed the sunspot-group daily data that were reported by Greenwich Photoheliographic Results (GPR) during the period 1874{\thinspace}--{\thinspace}1976 and Debrecen Photoheliographic Data (DPD) during the period 1977{\thinspace}--{\thinspace}2017. We determined the equatorial rotation rate [A{\$}A{\$}] and the latitude gradient [B{\$}B{\$}] components of the solar differential rotation by fitting the data in each of the 3-year moving time intervals (3-year MTIs) successively shifted by one year during the period 1874{\thinspace}--{\thinspace}2017 to the standard law of differential rotation. The values of A{\$}A{\$}and B{\$}B{\$}around the years of maxima and minima of Solar Cycles 12{\thinspace}--{\thinspace}24 are obtained from the 3-year MTIs series of A{\$}A{\$}and B{\$}B{\$}and studied the long-term cycle-to-cycle modulations in these coefficients. Here we have used the epochs of the maxima and minima of the Solar Cycles 12{\thinspace}--{\thinspace}24 that were recently determined from the revised Version-2 international sunspot-number series. We find that there exits a considerably significant secular decreasing-trend in A{\$}A{\$}around the maxima of solar cycles. There exist no secular trends in both A{\$}A{\$}and B{\$}B{\$}around the minima of solar cycles. The secular trend in B{\$}B{\$}around the maxima of solar cycles is also found to be statistically insignificant. We fitted a cosine function to the values of A{\$}A{\$}, and also to those of B{\$}B{\$}, after removing the corresponding linear trends. The cosine-fits suggest that there exist â‰ˆ54-year (â‰ˆ94-year) and â‰ˆ82-year (â‰ˆ79-year) periodicities in A{\$}A{\$}(B{\$}B{\$}) around the maxima and minima of solar cycles, respectively. The amplitude of the cosine-profile of A{\$}A{\$}(B{\$}B{\$}) around the minima of solar cycles is about 41{\%} (65{\%}) larger than that of A{\$}A{\$}(B{\$}B{\$}) around the maxima. In addition, the cosine profiles of A{\$}A{\$}and B{\$}B{\$}suggest a large (up to 180âˆ˜{\$}180^{\{}{\backslash}circ {\}}{\$}) phase difference between the long-term variations of A{\$}A{\$}, and also between those of B{\$}B{\$}, around maxima and minima of solar cycles. Implications of all these results are discussed briefly.},
issn={1573-093X},
doi={10.1007/s11207-020-01740-x},
url={https://doi.org/10.1007/s11207-020-01740-x}
}

